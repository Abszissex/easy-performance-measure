{
  "name": "easy-performance-measure",
  "version": "1.0.0",
  "description": "Super small wrapper methods for testing the performance of methods",
  "keywords": ["performance", "measure"],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "dev": "tsnd src/index.ts",
    "start": "node dist/index.js",
    "test": "jest --runInBand",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:write": "eslint \"src/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.2"
  },
  "author": "Pascal Zwikirsch",
  "license": "ISC",
  "homepage": "https://abszissex.medium.com/",
  "bugs": {
    "url": "https://github.com/Abszissex/easy-performance-measure/issues"
  }
}
